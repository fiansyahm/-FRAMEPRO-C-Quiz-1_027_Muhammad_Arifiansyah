@{
    ViewData["Title"] = "Products";
    ViewData["MyColor"] = "dark";
}

<div class="justify-content-center text-center">
    <h1>@ViewData["Title"]</h1>
</div>

<!-- Swiper -->
    <div class="swiper mySwiper mb-5 pb-5">
      <div class="swiper-wrapper">
            <div class="swiper-slide" style="margin-left:20%;">
                <div class="col-3 mt-3">
                    <div class="card border border-secondary border-5" style="width: 60vw;height: 30vw;object-fit: cover;background-image:url(https://img.freepik.com/free-vector/black-wallpaper-with-motion-lines-background_1017-30151.jpg)">
                      <div class="card-body">
                            <div class="row">
                                <div class="col-md-5">
                                    <div class="container start-50">
                                        <h3 class="text-success pt-5 pb-2">Selling Land</h3>
                                        <h5 class="text-white">Sell your land and we will help you to find A Great Buyer</h5>
                                        <a href="#" class="btn btn-outline-success mx-5 my-2">Let's Sell</a>
                                    </div>
                                </div>
                                 <div class="col-md-7 text-white">
                                     <img class="mt-4" src="https://cdn-cms.pgimgs.com/static/2021/08/1.-lahan-kosong.jpg" />
                                 </div>
                            </div>
                      </div>
                    </div>
                </div>
            </div>
             <div class="swiper-slide">
                <div class="col-3 mt-3">
                    <div class="card border border-secondary border-5" style="width: 60vw;height: 30vw;object-fit: cover;background-image:url(https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQZR3L1JoTAHk-3nQLwh0UdRMmkoSjmFBsuhBSeDELsUhS1C4R56NPWlbYKprmXp6gRQw4&usqp=CAU)">
                      <div class="card-body">
                            <div class="row">
                                <div class="col-md-5">
                                    <div class="container start-50">
                                        <h3 class="text-success pt-5 pb-2">Renting Out Land</h3>
                                        <h5 class="text-white">Rent out your land and we will help you to find A Great Tenant</h5>
                                        <a href="#" class="btn btn-outline-success mx-5 my-2">Let's Rent out</a>
                                    </div>
                                </div>
                                 <div class="col-md-7 text-white">
                                     <img class="mt-4" src="https://southasianad.com/wp-content/uploads/2020/05/v2.jpg" />
                                 </div>
                            </div>

                      </div>
                    </div>
                </div>
            </div>
             <div class="swiper-slide">
                <div class="col-3 mt-3">
                    <div class="card border border-black border-5" style="width: 60vw;height: 30vw;object-fit: cover;background-image:url(https://s3.envato.com/files/f0df8172-97ff-4a3e-a266-53d3def66fb1/inline_image_preview.jpg)">
                      <div class="card-body">
                            <div class="row">
                                <div class="col-md-5">
                                    <div class="container start-50">
                                        <h3 class="text-primary pt-5 pb-2">Buying Land</h3>
                                        <h5 class="text-black">We will help you to find A good Land and Best Seller</h5>
                                        <a href="#" class="btn btn-outline-primary mx-5 my-2">Let's Buy</a>
                                    </div>
                                </div>
                                 <div class="col-md-7 text-white">
                                     <img class="mt-4" src="https://cache.desktopnexus.com/thumbseg/2506/2506253-bigthumbnail.jpg" />
                                 </div>
                            </div>
                      </div>
                    </div>
                </div>
            </div>
             <div class="swiper-slide">
                <div class="col-3 mt-3">
                    <div class="card border border-black border-5" style="width: 60vw;height: 30vw;object-fit: cover;background-image:url(https://ak.picdn.net/shutterstock/videos/21019981/thumb/1.jpg)">
                      <div class="card-body">
                            <div class="row">
                                <div class="col-md-5">
                                    <div class="container start-50">
                                        <h3 class="text-primary pt-5 pb-2">Renting Land</h3>
                                        <h5 class="text-black">We will help you to find A Good Land for rent</h5>
                                        <a href="#" class="btn btn-outline-primary mx-5 my-2">Let's Rent</a>
                                    </div>
                                </div>
                                 <div class="col-md-7 text-white">
                                     <img class="mt-4" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRhlMZT0rfQ6byAIpmrQSoAaf0W1-53K9sz6w&usqp=CAU" />
                                 </div>
                            </div>
                      </div>
                    </div>
                </div>
            </div>
      </div>
      <div class="swiper-button-next"></div>
      <div class="swiper-button-prev"></div>
    </div>

    <!-- Swiper JS -->
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

    <!-- Initialize Swiper -->
    <script>
      var swiper = new Swiper(".mySwiper", {
        navigation: {
          nextEl: ".swiper-button-next",
          prevEl: ".swiper-button-prev",
        },
      });
    </script>


